<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-codelite-project">
    <comment>CodeLite Project</comment>
    <glob pattern="*.project"/>
    <sub-class-of type="application/xml"/>
  </mime-type>
  <mime-type type="application/x-codelite-workspace">
    <comment>CodeLite Workspace</comment>
    <glob pattern="*.workspace"/>
    <sub-class-of type="application/xml"/>
  </mime-type>
</mime-info>
